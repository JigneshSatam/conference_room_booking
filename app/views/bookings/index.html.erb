<div class="panel panel-default">
  <table class="table table-striped table-bordered table-hover table-condense">
		<tr>
			<th>Booking Id</th>
			<th>User Name</th>
		</tr>
		<% @bookings.each  do |booking|%>
			<tr>
				<td><%= booking.id %></td>
				<td><%= booking.user.name %></td>
			</tr>
		<% end %>
	</table>
</div>