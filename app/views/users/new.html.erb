<h4>Regester</h4>

<%= form_for @user, url: {action: 'create'}, role: "form" do |f| %>
	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>
	<div class="form-group">
		<%= f.label :email_id %>
		<%= f.text_field :email_id %>
	</div>
	<%= f.submit "Create", class: "btn btn-default" %>
<% end %>