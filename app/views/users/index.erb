<div class="panel panel-default">
  <table class="table table-striped table-bordered table-hover table-condense">
		<tr>
			<th>User Id</th>
			<th>User Name</th>
		</tr>
		<% @users.each  do |user|%>
			<tr>
				<td><%= user.id %></td>
				<td><%= user.name %></td>
			</tr>
		<% end %>
	</table>
</div>